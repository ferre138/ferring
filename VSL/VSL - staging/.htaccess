RewriteEngine On
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} !^www\.vsl3\.ca$ [NC]
RewriteRule ^(.*)$ https://www.vsl3.ca$1 [L,R=301]


ErrorDocument 404 /french/error404.html
ErrorDocument 403 /french/error403.html
ErrorDocument 300 /french/error404.html
ErrorDocument 500 /french/error500.html